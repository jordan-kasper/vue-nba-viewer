;AACA,0BAAA,0BADA;AAEA,6BAAA,2BAFA;AAGA,4BAAA;AAHA,SAJA;AASA,QAAA,MAAA,EAAA;AACA,UAAA,IAAA,EAAA;AADA;AATA,OAAA,CAAA,CAYA,IAZA,CAYA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,QAAA,CAAA,IAAA,CAAA,GAAA,CAAA,OAAA;AACA,OAdA;AAeA,KAlCA;AAmCA,IAAA,cAnCA,0BAmCA,IAnCA,EAmCA;AACA,WAAA,KAAA,CAAA,YAAA,EAAA,IAAA;AACA;AArCA,GAVA;AAiDA,EAAA,KAAA,EAAA;AACA,IAAA,MAAA,EAAA;AACA,MAAA,OADA,qBACA;AACA,aAAA,QAAA;AACA,aAAA,UAAA;AACA,OAJA;AAKA,MAAA,IAAA,EAAA,IALA;AAMA,MAAA,SAAA,EAAA;AANA;AADA,GAjDA;AA2DA,EAAA,QAAA,EAAA;AACA,IAAA,eADA,6BACA;AACA,UAAA,EAAA,GAAA,IAAA;AACA,UAAA,MAAA,GAAA,EAAA,CAAA,OAAA,CAAA,OAAA,CAAA,QAAA;;AACA,UAAA,MAAA,IAAA,IAAA,EAAA;AACA,eAAA,EAAA,CAAA,MAAA;AACA,OALA,CAMA;;;AACA,aAAA,EAAA,CAAA,MAAA,CAAA,MAAA,CAAA,UAAA,MAAA;AAAA,eAAA,MAAA,CAAA,QAAA,KAAA,MAAA;AAAA,OAAA,CAAA;AACA;AATA;AA3DA,CAAA","sourcesContent":["<template>\n  <div class=\"container\">\n    <div v-for=\"team in teamInfo\" v-bind:key=\"team.id\">\n      <div class=\"py-5 text-center\">\n        <img\n          :src=\"team.logo\"\n          width=\"150px\"\n          height=\"150px\"\n          style = \"margin-bottom:30px\"\n        />\n        <h2>{{ team.fullName }}</h2>\n        <p class=\"lead\">\n          Below is an example form built entirely with Bootstrap's form\n          controls. Each required form group has a validation state that can be\n          triggered by attempting to submit the form without completing it.\n        </p>\n      </div>\n\n      <div class=\"row\">\n        <div class=\"col-md-4 order-md-2 mb-4\">\n          <h4>Team Info</h4>\n          <p class=\"d-flex justify-content-between align-items-center mb-3\">\n            <b>TEAM DIVISION: </b> {{ team.leagues.standard.divName }}\n          </p>\n          <p class=\"d-flex justify-content-between align-items-center mb-3\">\n            <b>TEAM CONFERENCE:</b> {{ team.leagues.standard.confName }}\n          </p>\n          <p class=\"d-flex justify-content-between align-items-center mb-3\">\n            <b>TEAM CITY:</b> {{ team.city }}\n          </p>\n        </div>\n        <div class=\"col-md-8 order-md-1\">\n          <h4>2019 - 2020 Roster</h4>\n          <span v-for=\"player in players\" v-bind:key=\"player.id\">\n            <span v-if=\"player.leagues.standard != null && player.leagues.standard.active == 1\">\n                <img :src=\"'https://nba-players.herokuapp.com/players/' + player.lastName\" width=\"150px\" height=\"120px\"\n                @click=\"PlayerSelected(player.lastName)\"\n                style=\"padding-right: 20px; padding-top: 20px\"\n                :title=\"player.firstName + ' ' +player.lastName\">\n            </span>\n          </span>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  name: 'app',\n  props: ['teamId'],\n\n  data() {\n    return {\n      teamInfo: null,\n      players: null,\n    };\n  },\n  methods: {\n    teamCall() {\n      axios({\n        method: 'GET',\n        // eslint-disable-next-line prefer-template\n        url: 'https://api-nba-v1.p.rapidapi.com/teams/teamId/' + this.teamId,\n        headers: {\n          'content-type': 'application/octet-stream',\n          'x-rapidapi-host': 'api-nba-v1.p.rapidapi.com',\n          'x-rapidapi-key': 'gfetYKeN6VyxYZCQbYCF4TLt88QQnSoc',\n        },\n        params: {\n          page: '0',\n        },\n      }).then((response) => {\n        this.teamInfo = response.data.api.teams;\n      });\n    },\n    playerCall() {\n      axios({\n        method: 'GET',\n        // eslint-disable-next-line prefer-template\n        url: 'https://api-nba-v1.p.rapidapi.com/players/teamId/' + this.teamId,\n        headers: {\n          'content-type': 'application/octet-stream',\n          'x-rapidapi-host': 'api-nba-v1.p.rapidapi.com',\n          'x-rapidapi-key': 'gfetYKeN6VyxYZCQbYCF4TLt88QQnSoc',\n        },\n        params: {\n          page: '0',\n        },\n      }).then((response) => {\n        this.players = response.data.api.players;\n      });\n    },\n    PlayerSelected(name) {\n      this.$emit('playerPick', (name));\n    },\n  },\n  watch: {\n    $props: {\n      handler() {\n        this.teamCall();\n        this.playerCall();\n      },\n      deep: true,\n      immediate: true,\n    },\n  },\n  computed: {\n    filteredPlayers() {\n      const vm = this;\n      const league = vm.players.leagues.standard;\n      if (league != null) {\n        return vm.people;\n      }\n      // eslint-disable-next-line func-names\n      return vm.people.filter(person => person.category === league);\n    },\n  },\n};\n</script>\n<style scoped>\n.player {\n  padding-right: 10px;\n}\n</style>\n"],"sourceRoot":"src/components"}]}